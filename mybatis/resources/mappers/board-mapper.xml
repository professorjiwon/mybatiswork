<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="boardMapper">
	<select id="selectTotalRecord" resultType="_int">
		select count(*)
		  from board
		 where status = 'Y' 
	</select>
</mapper>